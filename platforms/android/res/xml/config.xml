<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.co.orbistech.hostedclient" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CacheClear">
        <param name="android-package" value="com.anrip.cordova.CacheClear" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Ping">
        <param name="android-package" value="org.tiste.cordova.ping.Ping" />
    </feature>
    <feature name="OneSignalPush">
        <param name="android-package" value="com.plugin.gcm.OneSignalPush" />
    </feature>
    <feature name="RemoteInjection">
        <param name="android-package" value="com.truckmovers.cordova.RemoteInjectionPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>hostedClient</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://192.168.16.35:5000/" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
